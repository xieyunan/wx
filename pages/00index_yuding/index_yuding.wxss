/* pages/00index_yuding/index_yuding.wxss */
@import '../style/weui.wxss';
.yuding_wrap{
  font-size: 28rpx;
  font-family: "微软雅黑";
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}
input{
  height: 40rpx;
  min-height: 40rpx;
}
.weui-btn{
  margin: 0;
}
.weui-cell{
  padding: 30rpx 0;
  margin: 0 30rpx;
  font-size: 28rpx;
  font-family: "微软雅黑";
  border-bottom: 1px solid #e3e3e3;
}
.weui-cell__ft{
  letter-spacing: 2rpx;
}
.input-placeholder{
  font-size: 28rpx;
  font-family: "微软雅黑";
}
.phone,.wenben{
  border: none;
}

.picker_border{
  border: 3px solid #ff253d;
}
.yuding_table{
  color: #ff253d;
  font-weight: 600;
  margin-left: 30rpx; 
}
.navbar_wrap{
  position: relative;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  font-family: "微软雅黑";
}
.navbar_item{
  width: 50%;
  height: 100rpx;
}
.navbar_item_title{
  width: 215rpx;
  margin: 0 auto;
  height: 100%;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #999;
}
.item_active{
  color: #ff253d;
  border-bottom: 2px solid #ff253d;
}
.caipin{
  margin-bottom: 20rpx;
}
.buchong{
  
}
/* 席位内容 */
.xiwei_wrap{
  font-family: "微软雅黑";
}
.xianyou{
  margin-top: 10rpx;
  padding: 0 30rpx;  
  background: #fff;
  margin-bottom: 20rpx;
}
.xianyou_title{
  font-size: 30rpx;
  line-height: 30rpx;
  padding: 20rpx 0;
  border-bottom: 1px solid #ccc;
}
.zhuowei_item,.baoxiang_item{
  display: inline-block;
  color: #fff;
  width: 200rpx;
  height: 56rpx;
  font-size: 24rpx;
  line-height: 66rpx;
  padding: 15rpx 0;
  margin-right: 30rpx;
  background-image:url('data:image/png;base64,'); 
  background-size: 100% 100%;
  text-indent: 40rpx;

}
.xianyou_main{
  padding: 20rpx 0;
}
.zhuowei{
  margin-bottom: 20rpx ;
}
.baoxiang_item{
  width: 115rpx;
  height: 85rpx;
  background: #ffaf25;
  border-radius: 10rpx;
  line-height: 40rpx;
}
.kongwei_scroll{
  white-space: nowrap;
  display: flex;
  overflow : auto;
}
.zhuowei_menu{
  background: #fff;
  margin-bottom: 20rpx;
  display: flex;
  padding: 20rpx 30rpx;
  line-height: 50rpx;
}
.zhuowei_menu>input{
  width: 340rpx;
  padding: 0 30rpx;
  color: #ff253d;
}
.zhuowei_item_active{
  background: rgba(230, 230, 230, 0.5)
}
.submit{
  height: 80rpx;
  line-height: 80rpx;
}
.input_last{
  margin-bottom: 20rpx;
}
.buchong{
  margin-bottom: 120rpx;
}