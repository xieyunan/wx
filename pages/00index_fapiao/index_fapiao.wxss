/* pages/00index_fapiao/index_fapiao.wxss */
@import '../style/weui.wxss';
.fapiao_wrap{
  color: #999;
  font-family: "微软雅黑";
}
.fapiao_personal{
  margin-top: 10rpx;
  background: #fff;
  display: flex;
  position: relative;
  justify-content: space-between;
  padding: 15rpx 25rpx;
  line-height: 55rpx;
  box-shadow:  0 10px 10px rgba(230, 230, 230, 0.5);
  z-index: 5;
}
.fapiao_img>image{
  width: 36rpx;
  height: 36rpx;

}
.fapiao_down>image{
  width: 32rpx;
  height: 31rpx;
}
.fapiao_img{
  display: flex;
  position: relative;
  width: 300rpx;
  height: 100%;
}
.gou{
  position: absolute;
  top: 2rpx;
  left: 5rpx;
}
.yuanquan{
  display: inline-block;
  margin-top: 10rpx;
  margin-right: 20rpx;
}
.personal_name{
  line-height: 55rpx;
  margin: 10rpx 25rpx 40rpx 25rpx;
  padding: 10rpx;
  background: #fff;
  color: #000;
}
.name_input{
  display: flex;
  padding: 10rpx 0 ;
}
.name{
  border-bottom: 1px solid #ccc;
}
input{
  padding: 0 20rpx;
  width: 500rpx;
  margin-top: 6rpx;
  text-align: center;
  color: green;
  font-size: 26rpx;
}
.active{
  color: #000;
}
.qiye{
  margin-top: 20rpx;
}
